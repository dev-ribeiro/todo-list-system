{
  "name": "xneo/app",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "start:dev": "php -S localhost:8000 -d PHP_ENV=dev",
    "start:prod": "php -S localhost:8000 -d PHP_ENV=prod",
    "phinx": "vendor/bin/phinx",
    "migrate:dev": "vendor/bin/phinx migrate --configuration=phinx.development.php",
    "migrate:prod": "vendor/bin/phinx migrate --configuration=phinx.php",
    "load": "composer dump-autoload -o"
  },
  "config": {
    "process-timeout": 0
  },
  "require-dev": {
    "robmorgan/phinx": "^0.13.4"
  }
}
